extends base

block title 
	p User list

block left

block main
	table
		tr
			td Nickname
			td Username
			td Registration date
		- foreach( user; booru.users.getAll() )
			tr
				td 
					a(href='user_profile/#{ user.nickname }') #{ user.nickname }
				td #{ user.name }
				td #{ user.registration_datetime.toSimpleString() }
